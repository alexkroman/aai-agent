{
  "name": "@aai-agent/react",
  "version": "0.1.0",
  "description": "React hooks and components for aai-agent voice assistants",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.39.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.5.0",
    "jsdom": "^28.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stylelint": "^17.4.0",
    "stylelint-config-standard": "^40.0.0",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "build": "vite build && vite build --config vite.config.iife.js",
    "build:lib": "vite build",
    "build:iife": "vite build --config vite.config.iife.js",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ && stylelint \"src/**/*.css\"",
    "lint:fix": "eslint src/ --fix && stylelint \"src/**/*.css\" --fix"
  },
  "dependencies": {}
}
