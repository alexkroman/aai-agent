{
  "name": "aai-voice-platform",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsc --noEmit && tsx watch src/index.ts",
    "dev:serve": "tsc --noEmit && node scripts/build-client.js && CLIENT_DIR=dist tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc && node scripts/build-client.js",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ client/__tests__/",
    "format": "prettier --check src/ client/__tests__/",
    "format:fix": "prettier --write src/ client/__tests__/",
    "check": "npm run typecheck && npm run lint && npm run format && npm run test -- run"
  },
  "dependencies": {
    "isolated-vm": "^6.0.2",
    "number-to-words": "^1.2.4",
    "pino": "^10.3.1",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.3.2",
    "@types/number-to-words": "^1.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.2",
    "jsdom": "^28.1.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  }
}
